# Example .env for local development
# Set DB_KIND to POSTGRES to use Postgres instead of H2 in development
# Alternatively use H2 (default): DB_KIND=H2
DB_KIND=POSTGRES
DB_URL=jdbc:postgresql://localhost:5432/pedidos
DB_USER=postgres
DB_PASS=postgres

# --- Entorno Spring ---
SPRING_PROFILES_ACTIVE=dev
